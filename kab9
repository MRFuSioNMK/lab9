#include <iostream>
#include <string>

using namespace std;

bool containsSubstring(const string& S, const string& S0) {
    size_t found = S.find(S0);
    return found != string::npos;
}

int main() {
    string S = "Hello, world!";
    string S0 = "world";

    if (containsSubstring(S, S0)) {
        cout << "True" << endl;
    } else {
        cout << "False" << endl;
    }

    return 0;
}
